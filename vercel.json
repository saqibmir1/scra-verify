{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://scraverify-production.up.railway.app/:path*"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "https://scraverify-production.up.railway.app",
    "NEXT_PUBLIC_SITE_URL": "https://scra-verify.vercel.app"
  }
}